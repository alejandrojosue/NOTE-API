      "required": true
    },
    "cantidad": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "isv": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "precio": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "descuentoValor": {
      "type": "decimal",
      "required": false,
      "min": 0
    }
  }
}
