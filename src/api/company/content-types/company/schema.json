{
  "kind": "collectionType",
  "collectionName": "companies",
  "info": {
    "singularName": "company",
    "pluralName": "companies",
    "displayName": "Company"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "rtn": {
      "type": "string",
      "minLength": 14,
      "maxLength": 14,
      "required": true,
      "unique": false
    },
    "name": {
      "type": "string"
    },
    "manager": {
      "type": "string"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "contability": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::contability.contability"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "address": {
      "type": "text"
    },
    "phones": {
      "type": "string"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "email": {
      "type": "email"
    },
    "slogan": {
      "type": "string"
    }
  }
}
